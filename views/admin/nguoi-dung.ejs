<%- include Dheader -%>
    <div class="col-lg-8">
        <h2 id="01">Người Dùng</h2>
        <table class="table text-center">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">FullName</th>
                    <th scope="col">Job</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0;i<=10;i++){ %>
                <tr>
                    <th scope="row"><%- i %></th>
                    <td>Mark</td>
                    <% if(i==0){ %>
                        <td>Amin</td>
                    <% }else{ %>
                        <% if(i%2==0){ %>
                            <td>Writer</td>
                        <% }else{ %>
                            <% if(i%3==0){ %>
                                <td>Editor</td>
                            <% }} %>
                    <% }%>
                    <% if(i==1 || i==5 || i==7){ %>
                        <td>Subcriber</td>
                    <% } %>
                </tr>
                <% } %>
            </tbody>
        </table>
        <h2 id="02">Người Dùng Vip</h2>
        <table class="table text-center">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">FullName</th>
                    <th scope="col">Job</th>
                    <th scope="col">day left</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0;i<=10;i+=2){ %>
                <tr>
                    <th scope="row"><%- i %></th>
                    <td>Mark</td>
                    <% if(i==0){ %>
                        <td>Amin</td>
                    <% }else{ %>
                        <% if(i%2==0){ %>
                            <td>Writer</td>
                        <% }else{ %>
                            <% if(i%3==0){ %>
                                <td>Editor</td>
                            <% }} %>
                    <% }%>
                    <% if(i==1 || i==5 || i==7){ %>
                        <td>Subcriber</td>
                    <% } %>
                    <td><%- i %></td>
                </tr>
                <% } %>
            </tbody>
        </table>
        <h2 id="03">Editor</h2>
        <table class="table text-center">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">FullName</th>
                    <th scope="col">chuyên mục</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0;i<=6;i++){ %>
                <tr>
                    <th scope="row"><%- i %></th>
                    <td>Mark</td>
                    <td>rau củ</td>
                </tr>
                <% } %>
            </tbody>
        </table>
        <h2 id="04">Writer</h2>
        <table class="table text-center">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">FullName</th>
                    <th scope="col">Blog</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0;i<=3;i++){ %>
                <tr>
                    <th scope="row"><%- i %></th>
                    <td>Mark</td>
                    <td>1243</td>
                </tr>
                <% } %>
            </tbody>
        </table>
    </div>
    <div class="col-lg-2">
        <div style="position:sticky">
            <ul class="nav nav-pills flex-column">
                <li class="nav-item" style="margin-bottom: 3px;">
                    <a href="#01">Người Dùng</a>
                </li>
                <li class="nav-item" style="margin-bottom: 3px;">
                    <a href="#02">Người Dùng Vip</a>
                </li>
                <li class="nav-item" style="margin-bottom: 3px;">
                    <a href="#03">Editor</a>
                </li>
                <li class="nav-item" style="margin-bottom: 3px;">
                    <a href="#04">Writer</a>
                </li>
            </ul>
        </div>
    </div>
<%- include Dfooter -%>    